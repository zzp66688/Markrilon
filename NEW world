class Block {
    
    public index: number;
    public hash: string;
    public previousHash: string;
    public timestamp: number;
    public date: string;


    cnostructor(index: number,hash: string,previousHash: string,timestamp: number,date: string)
     {  this.index = index;
        this.hash = hash;
        this.previousHash = previousHash;
        this.timestamp = timestamp;
        this.date = date;
     }
}
